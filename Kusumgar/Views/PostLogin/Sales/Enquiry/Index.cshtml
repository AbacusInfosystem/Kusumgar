                                                                <tr>
                                                                    <th>Quality Attributes</th>
                                                                    <th>Values</th>
                                                                </tr>

                                                                <tr>
                                                                    <td>Yarn Type</td>
                                                                    <td>Nylon 66</td>
                                                                </tr>

                                                                <tr>
                                                                    <td>
                                                                        Reed
                                                                    </td>
                                                                    <td>50</td>
                                                                </tr>

                                                                <tr>
                                                                    <td>
                                                                        Pick
                                                                    </td>
                                                                    <td>62</td>
                                                                </tr>

                                                                <tr>
                                                                    <td>
                                                                        Weave
                                                                    </td>
                                                                    <td>62</td>
                                                                </tr>

                                                                <tr>
                                                                    <td>
                                                                        Minimum Order Size
                                                                    </td>
                                                                    <td>62</td>
                                                                </tr>

                                                                <tr>
                                                                    <td>
                                                                        Ideal Roll Length
                                                                    </td>
                                                                    <td>62</td>
                                                                </tr>

                                                                <tr>
                                                                    <td>
                                                                        Our Sample No
                                                                    </td>
                                                                    <td>62</td>
                                                                </tr>
                                                            </table>

                                                        </div>

                                                        <div class="form-group">
                                                            <label for="chkStatus">Status :</label><br>
