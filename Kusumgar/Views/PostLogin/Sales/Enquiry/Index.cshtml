                                                            <input type="hidden" class="iCheck-hidden" id="hdnStatus" value="@((Model.Enquiry.Is_Active == null ? false : Model.Enquiry.Is_Active).ToString())">
                                                        </div>

                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="box-footer">
                                            <div class="btn-group">
                                                <button type="button" id="btnSave_Enquiry" class="btn btn-success btn-sm">Save</button>
                                                <a href="@Url.Action("Search", "Enquiry")" id="btnCancel" class="btn btn-warning btn-sm" role="button">Cancle</a>
                                            </div>
                                        </div>
