                        <li><a href="#tab_5" id="tabFuncational_Visual" data-toggle="tab">Functional / Visual</a></li>
                        <li class="pull-right"><a href="#" class="text-muted"><i class="fa fa-info"></i></a></li>
                    </ul>

                    <div class="tab-content">

                        <div class="tab-content">
                            <div class="tab-pane active" id="tab_1">
                                <form id="frmEnquiry_Prerequisites">
                                    <div id="divEnquiryPrerequisites">
                                        <div class="row">
                                            <div class="col-md-6">
                                                <div class="box-body">
                                                    <div class="container-fluid">
                                                        <div class="row">
                                                            @foreach (var item in LookupInfo.Get_Enquiry_Types())
                                                            {
                                                                if (Model.Enquiry.Enquiry_Type_Id == item.Key)
                                                                {
                                                                    <div class="col-md-4 col-xs-4">
                                                                        <input type="radio" name="Enquiry_Type" id="rd_@item.Value" checked="checked" value="@item.Key" />
                                                                        &nbsp; @item.Value
                                                                    </div>
                                                                }
                                                                else
                                                                {
                                                                    <div class="col-md-4 col-xs-4">
                                                                        <input type="radio" name="Enquiry_Type" id="rd_@item.Value" value="@item.Key" />
                                                                        &nbsp; @item.Value
                                                                    </div>
                                                                }
                                                            }
                                                            <input type="hidden" id="hdnEnquiry_Type_Id" value="@Model.Enquiry.Enquiry_Type_Id" name="Enquiry.Enquiry_Type_Id">
                                                        </div>
                                                        <br />

                                                        <div class="form-group">
                                                            <label style="color: #333" for="txtCustomer_Name">Customer name</label>
                                                            <div class="auto-complete">
                                                                <input type="text" class="form-control input-sm" id="txtCustomer_Name" placeholder="Enter customer name" value="@Model.Enquiry.Customer_Name">
                                                                <input type="hidden" id="hdnCustomer_Id" value="@(Model.Enquiry.Customer_Id == 0 ? "" : Model.Enquiry.Customer_Id.ToString())" name="Enquiry.Customer_Id" class="auto-complete-value">
                                                                <input type="hidden" id="hdnCustomer_Name" value="@Model.Enquiry.Customer_Name" name="Enquiry.Customer_Name" class="auto-complete-label">
                                                            </div>
                                                        </div>

                                                        <div class="form-group">
                                                            <label style="color: #333" for="txtQuality_No">Quality name</label>
                                                            <div class="auto-complete">
                                                                <input type="text" class="form-control input-sm" id="txtQuality_No" placeholder="Enter quality code" value="@Model.Enquiry.Quality_No">
                                                                <input type="hidden" id="hdnQuality_Id" value="@(Model.Enquiry.Quality_Id == 0 ? "" : Model.Enquiry.Quality_Id.ToString())" name="Enquiry.Quality_Id" class="auto-complete-value">
                                                                <input type="hidden" id="hdnQuality_No" value="@Model.Enquiry.Quality_No" name="Enquiry.Quality_No" class="auto-complete-label">
                                                            </div>
                                                        </div>

                                                        <div class="box-body table-responsive no-padding">
