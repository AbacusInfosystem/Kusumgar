                                        <div class="box-body">
                                            <div class="row">
                                                <div class="col-md-4 col-xs-4">
                                                    <div class="form-group">
                                                        <label for="txtQuantity" style="color: #333">Quantity</label>
                                                        <div class="input-group">
                                                            @if (Model.Enquiry.Staggered_Order.Quantity != 0)
                                                            { 
                                                                <input type="text" id="txtQuantity" placeholder="Enter Quantity" class="form-control input-sm" value="@Model.Enquiry.Staggered_Order.Quantity" name="Enquiry.Staggered_Order.Quantity" />
                                                            }
                                                            else
                                                            {
                                                                <input type="text" id="txtQuantity" placeholder="Enter Quantity" class="form-control input-sm"  name="Enquiry.Staggered_Order.Quantity" />
                                                            }
                                                            
                                                            <span class="input-group-addon">Meters</span>
                                                        </div>
                                                    </div>
                                                    <div class="form-group">
                                                        <label for="DtDelivery_Date" style="color: #333">Delivery Date</label>
                                                        <div class="input-group">
                                                            <div class="input-group-addon">
                                                                <i class="fa fa-calendar"></i>
                                                            </div>
