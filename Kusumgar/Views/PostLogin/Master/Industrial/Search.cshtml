@{
    ViewBag.Title = "Search";
}

<link rel="stylesheet" href="~/Content/jquery-ui.css">
<input type="hidden" id="hdfCurrentPage" name="Pager.CurrentPage" value="@Model.Pager.CurrentPage" />
<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
    @{Html.RenderPartial("_ServerMessage");}
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            Industrial

            <small>Search</small>
        </h1>

        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i>Home</a></li>
            <li>Masters</li>
            <li class="active">Industrial Search</li>
        </ol>
    </section>
    <form id="frmSearch_comp">
        @*<input type="hidden" id="hdnComplaint_Id" name="ComplaintId">*@
        <!-- Main content -->
        <section class="content">
            <div class="row">
                <div class="col-md-12">
                    <div class="box box-primary">
                        <!-- form start -->
                        <div class="box-body">
                            @*<div class="row">
                                <div class="col-md-12">
                                    <div class="form-group">
                                        <label for="txtCustName">Customer Name</label>
                                        <input type="text" class="form-control" id="txtCustName" placeholder="Enter Customer Name" />
                                    </div>
                                </div>
                            </div>*@

                            <div class="box-footer">
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="btn-group pull-left">
                                            @*<button type="button" id="btnSearch" class="btn btn-success">Search</button>*@
                                            <a class="btn btn-primary" href="@Url.Action("Index","Industrial")" role="button">Industrial Master Creation </a>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="btn-group pull-right">
                                            <button type="button" id="btnEdit" style="display:none;" class="btn btn-warning">Edit Industrial Master</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <!-- /.content -->
            <div class="row">
                <div class="col-md-12">
                    <div class="box box-solid">
                        <div class="box-body table-responsive no-padding">
                            <table class="table table-striped" id="tblComGrid">
                                <tr id="subhead">
                                    <th style="width:50px;"></th>
                                    <th style="width:300px;">Industrial Category Name</th>
                                    <th style="width:100px;">Industrial Group Name</th>
                                    <th style="width:100px;">Industrial Sub-Group Name</th>
                                    <th style="width:100px;">Size</th>
                                    <th style="width:300px;">COD</th>
                                </tr>
                            </table>
                        </div>
                        <div class="box-footer clearfix">
                            <div class="pagination pagination-sm no-margin pull-right">
                                <ul></ul>
                            </div>
                            @*<div id="divSearchGridOverlay" class="overlay">
                                    <i class="fa fa-refresh fa-spin"></i>
                                </div>*@
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </form>
</div>
<!-- /.content-wrapper -->
