
<style>
    .example-modal .modal {
        position: relative;
        top: auto;
        bottom: auto;
        right: auto;
        left: auto;
        display: block;
        z-index: 1;
    }

    .example-modal .modal {
        background: transparent!important;
    }
</style>

<script src="~/Scripts/Master/Event/test_unit_search_event.js"></script>
<script src="~/Scripts/Master/Method/test_unit_search_method.js"></script>

<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
    @{Html.RenderPartial("_ServerMessage");}
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>Test Unit
           
            <small>Search</small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i>Home</a></li>
            <li>Master</li>
            <li class="active">Test Unit Search</li>
        </ol>
    </section>

    <!-- Main content -->
    <section class="content">
        <!-- Customer Search -->
        <div class="row">
            <!-- left column -->
            <div class="col-md-12">
                <div class="box box-primary">
                    <!-- form start -->

                    <form id="frmTestUnitSearch" method="post">
                        <div class="box-body">

                            <div class="row">
                                <div class="col-md-12">

                                    <div class="form-group">
                                       
                                     <input type="text" name="Filter.Test_Unit_Name" class="form-control" id="txtTestUnitName" placeholder="Enter Test Unit Name">
                                       
                                    </div>
                                </div>

                            </div>

                            <div class="box-footer">
                                <div class="row">
                                    <div class="col-md-6">

                                        <div class="btn-group pull-left">
                                            <button type="button" id="btnSearch" class="btn btn-success btn-sm">Search</button>
                                            <a class="btn btn-primary btn-sm" href="@Url.Action("Index","TestUnit")" role="button">Create</a>
                                        </div>

                                    </div>
                                    <div class="col-md-6">

                                        <div class="btn-group pull-right">

                                            <input type="hidden" id="hdfCurrentPage" name="Pager.CurrentPage" value="@Model.Pager.CurrentPage" />
                                         
                                            <input type="hidden" id="hdfTestUnitId" name="Edit_Mode.Test_Unit_Id" />

                                            <button type="button" class="btn btn-warning btn-sm" id="btnEdit" style="display:none;">Edit</button>
                                            <button type="button" class="btn btn-primary btn-sm" id="btnView">View Test</button>
                                        </div>

                                    </div>
                                </div>
                            </div>

                        </div>
                          <!-- /.box-body -->
                        </form>
                  </div>
            </div>
        </div>
        <!-- Customer Search Ends -->

        <div class="row">
            <div class="col-md-12">
                <div class="box box-solid">
                    <div class="box-body table-responsive no-padding">

                        <table class="table table-striped" id="tblSearchTestUnit">
                            <tr>
                                <th style="width:50px;"></th>
                                <th>Test Unit</th>
                                <th style="width:100px;">Status</th>
                            </tr>
                         
                        </table>

                    </div>
                    <!-- /.box-body -->
                    <div class="box-footer clearfix">
                        <div class="pagination pagination-sm no-margin pull-right">
                            <ul>
                               
                            </ul>
                            </div>
                        </div>

                    <div id="divSearchGridOverlay" class="overlay">
                        <i class="fa fa-refresh fa-spin"></i>
                    </div>
          

                </div>
                <!-- /.box -->
            </div>
            <!-- /.col (left) -->
        </div>

       
    </section>
    <!-- /.content -->
</div>
<!-- /.content-wrapper -->

<script type="text/javascript">

    $(document).ready(function () {

        $('input').iCheck({
            checkboxClass: 'icheckbox_square-green',
            radioClass: 'iradio_square-green',
            increaseArea: '20%' // optional
        });

        $("#btnSearch").click(function () {
            $("#divSearchGridOverlay").show();
        });

        $("#divSearchGridOverlay").hide();
    });
</script>
