@{
    ViewBag.Title = "Customer";
}


<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">

    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>Process
           
            <small>Master</small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i>Home</a></li>
            <li>Master</li>
            <li class="active">Process</li>
        </ol>
    </section>

    <!-- Main content -->
    <section class="content">
        <div class="row">
            <div class="col-md-12">

                <div class="box box-primary">
                    <!-- form start -->
                    <div class="box-body">

                        <div class="row">
                            <div class="col-md-4">

                                <div class="form-group">
                                    <label for="txtProcessName">Process name</label>
                                    <input type="text" class="form-control" id="txtProcessName" placeholder="Enter process name">
                                </div>

                                <div class="form-group">
                                    <label>
                                        <input type="checkbox" name="chkIsApproved" checked />
                                        &nbsp;&nbsp;&nbsp;&nbsp; Approved Status
                                   
                                    </label>
                                </div>

                            </div>
                        </div>

                        <div class="box-footer">
                            <button type="submit" id="btnSearch" class="btn btn-success">Save</button>
                            <a href="@Url.Action("Search","Process")" id="btnCancel" class="btn btn-warning" role="button">Cancle and Open Search</a>
                            
                        </div>

                    </div>
                    <!-- /.box-body -->
                </div>
            </div>
            <!-- /.col -->
        </div>
        <!-- /.row -->
    </section>
    <!-- /.content -->
</div>
<!-- /.content-wrapper -->

<script type="text/javascript">

    $(document).ready(function () {

        $('input').iCheck({
            checkboxClass: 'icheckbox_square-green',
            radioClass: 'iradio_square-green',
            increaseArea: '20%' // optional
        });
    });
</script>
